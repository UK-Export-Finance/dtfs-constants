{
  "name": "@UK-Export-Finance/dtfs-constants",
  "version": "0.0.2",
  "description": "This repo contains a set of constants used in the DTFS project",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanup",
    "package": "npm run build && npm pack",
    "addscope": "node tools/packagejson name @UK-Export-Finance/dtfs-constants"
  },
  "publishConfig": {
    "@UK-Export-Finance:registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Gabriel Ignat",
  "license": "MIT",
  "homepage": "https://github.com/UK-Export-Finance/dtfs-constants#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs-constants.git"
  },
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs-constants/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
